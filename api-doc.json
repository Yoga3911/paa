{
    "openapi": "3.0.2",
    "info": {
        "title": "API",
        "version": "1.0"
    },
    "servers": [
        {
            "url": "https://api.test/api/v1"
        }
    ],
    "paths": {
        "/auth/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Login",
                "description": "Pengguna login menggunakan Email dan Password",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "required": [
                                            "true"
                                        ],
                                        "nullable": false,
                                        "default": "mamat@gmail.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "required": [
                                            "true"
                                        ],
                                        "nullable": false,
                                        "default": "123456"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "object",
                                            "properties": {
                                                "id": {
                                                    "type": "integer",
                                                    "default": 1
                                                },
                                                "name": {
                                                    "type": "string",
                                                    "default": "Mamat"
                                                },
                                                "email": {
                                                    "type": "string",
                                                    "default": "mamat@gmail.com"
                                                },
                                                "password": {
                                                    "type": "string",
                                                    "default": "$2a$04$YIA154Hs0l6gcuwDtEKTw.USIJNJmgZFRt31mHGaqE.4c5FPDO3bq"
                                                },
                                                "gender_id": {
                                                    "type": "integer",
                                                    "format": "int16",
                                                    "default": 1
                                                },
                                                "role_id": {
                                                    "type": "integer",
                                                    "format": "int16",
                                                    "default": 1
                                                },
                                                "image": {
                                                    "type": "string",
                                                    "default": "https://firebasestorage.googleapis.com/v0/b/test-34a2a.appspot.com/o/1645155203449-5427259544859421852_121.jpg?alt=media&token=32244b2b-288f-42e7-813c-3549bdd8487b"
                                                }
                                            }
                                        },
                                        "message": {
                                            "default": "Login berhasil!",
                                            "type": "string"
                                        },
                                        "status": {
                                            "type": "boolean",
                                            "default": true
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "object",
                                            "default": null
                                        },
                                        "message": {
                                            "type": "string",
                                            "default": "Email atau Password salah"
                                        },
                                        "status": {
                                            "type": "boolean",
                                            "default": false
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "object",
                                            "default": null
                                        },
                                        "message": {
                                            "type": "string",
                                            "default": "Internal Server Error"
                                        },
                                        "status": {
                                            "type": "boolean",
                                            "default": false
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/auth/register": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Register",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "default": "Mamat"
                                    },
                                    "email": {
                                        "type": "string",
                                        "default": "mamat@gmail.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "default": "123456"
                                    },
                                    "gender_id": {
                                        "type": "integer",
                                        "format": "int16",
                                        "default": 1
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "object",
                                            "default": null
                                        },
                                        "message": {
                                            "type": "string",
                                            "default": "Register berhasil!"
                                        },
                                        "status": {
                                            "type": "boolean",
                                            "default": true
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "object",
                                            "default": null
                                        },
                                        "message": {
                                            "type": "string",
                                            "default": "Nama atau Email sudah terdaftar"
                                        },
                                        "status": {
                                            "type": "boolean",
                                            "default": false
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "object",
                                            "default": null
                                        },
                                        "message": {
                                            "type": "string",
                                            "default": "Internal Server Error"
                                        },
                                        "status": {
                                            "type": "boolean",
                                            "default": false
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/user/profile/{userId}": {
            "get": {
                "tags": [
                    "User"
                ],
                "responses": {
                    "200": {
                        "description": ""
                    }
                }
            }
        }
    }
}